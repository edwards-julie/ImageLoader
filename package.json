{
  "name": "imageloader",
  "version": "0.0.1",
  "scripts": {
    "watch": "npm-watch",
    "build": "react-scripts build",
    "start": "react-scripts start"
  },
  "dependencies": {
    "npm-watch": "^0.7.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "^4.0.1"
  },
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "extensions": "js,jsx"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
